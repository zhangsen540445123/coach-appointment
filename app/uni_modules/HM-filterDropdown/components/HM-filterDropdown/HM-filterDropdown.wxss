.HMfilterDropdown {
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  flex-shrink: 0;
  flex-wrap: nowrap;
  left: 0;
  overflow-y: hidden;
  top: 0;
  width: 100%;
  z-index: 2
}

.HMfilterDropdown.setDropdownBottom {
  bottom: 0
}

.HMfilterDropdown view {
  display: -webkit-flex;
  display: flex;
  flex-wrap: nowrap
}

.region {
  flex: 1;
  height: 44px
}

.nav {
  background-color: #fff;
  justify-content: space-between;
  overflow: hidden;
  padding: 24rpx 36rpx;
  position: relative;
  width: 100%;
  z-index: 12
}

.nav,
.nav .first-menu {
  display: -webkit-flex;
  display: flex;
  flex-direction: row
}

.nav .first-menu {
  align-items: center;
  background: #f5f5f5;
  border-radius: 12rpx;
  color: #757575;
  color: rgba(0, 0, 0, .5);
  font-family: AppleSystemUIFont;
  font-size: 13px;
  font-size: 26rpx;
  font-weight: 400;
  height: 64rpx;
  justify-content: center;
  line-height: 30.4rpx;
  mix-blend-mode: normal;
  text-align: center;
  transition: color .2s linear;
  width: 180.4rpx
}

.nav .first-menu.on {
  background: transparent
}

.nav .first-menu .name {
  overflow: hidden;
  text-align: center;
  text-overflow: clip
}

.nav .first-menu>._i {
  align-items: center;
  height: 13px;
  justify-content: center;
  margin-left: 4rpx;
  transition: color .2s linear, -webkit-transform .2s linear;
  transition: transform .2s linear, color .2s linear;
  transition: transform .2s linear, color .2s linear, -webkit-transform .2s linear;
  width: 13px
}

.sub-menu-class {
  background-color: #fff;
  box-shadow: 0 5px 5px rgba(0, 0, 0, .1);
  flex-direction: row;
  max-height: 884rpx;
  overflow: hidden;
  transition: -webkit-transform .15s linear;
  transition: transform .15s linear;
  transition: transform .15s linear, -webkit-transform .15s linear;
  width: 100%;
  z-index: 11
}

.sub-menu-class.hide {
  display: none
}

.sub-menu-list {
  height: 345px;
  width: 100%
}

.sub-menu-list,
.sub-menu-list .sub-menu {
  flex-direction: column
}

.sub-menu-list .sub-menu {
  font-size: 13px;
  min-height: 44px;
  padding-right: 15px
}

.sub-menu-list .sub-menu>.menu-name {
  align-items: center;
  flex-direction: row;
  height: 44px;
  justify-content: space-between
}

.sub-menu-list .sub-menu>.menu-name>.iconfont {
  color: var(--common-pink-color);
  display: none;
  font-size: 18px
}

.sub-menu-list.first {
  background-color: #f0f0f0;
  box-shadow: 5rpx 0 5rpx rgba(0, 0, 0, .1);
  flex-shrink: 0;
  width: 236rpx
}

.sub-menu-list.first .sub-menu {
  padding-left: 15px
}

.sub-menu-list.first .sub-menu.on {
  background-color: #fff
}

.sub-menu-list.alone {
  height: auto;
  max-height: 345px;
  min-height: 170px
}

.sub-menu-list.alone .sub-menu {
  border-bottom: 1rpx solid #e5e5e5;
  margin-left: 15px;
  min-height: calc(44px - 1rpx)
}

.sub-menu-list.alone .sub-menu.on {
  color: var(--common-pink-color)
}

.sub-menu-list.alone .sub-menu.on>.menu-name>.iconfont {
  display: block
}

.sub-menu-list.not-first {
  box-shadow: 5rpx 0 5rpx rgba(0, 0, 0, .1)
}

.sub-menu-list.not-first .sub-menu {
  border-bottom: 1rpx solid #e5e5e5;
  margin-left: 15px;
  min-height: calc(44px - 1rpx)
}

.sub-menu-list.not-first .sub-menu>.menu-name {
  height: calc(44px - 1rpx)
}

.sub-menu-list.not-first .sub-menu>.menu-name>.iconfont {
  color: var(--common-pink-color);
  display: none;
  font-size: 18px
}

.sub-menu-list.not-first .sub-menu.on {
  color: var(--common-pink-color)
}

.sub-menu-list.not-first .sub-menu.on>.menu-name>.iconfont {
  display: block
}

.sub-menu-list.not-first .sub-menu .more-sub-menu {
  flex-direction: row;
  flex-wrap: wrap;
  padding-bottom: 9px
}

.sub-menu-list.not-first .sub-menu .more-sub-menu>text {
  background-color: #f5f5f5;
  border: 1rpx solid #f5f5f5;
  border-radius: 3px;
  color: #9b9b9b;
  flex: 0 0 calc(33.33% - 6px);
  font-size: 12px;
  height: 30px;
  line-height: 30px;
  margin-bottom: 6px;
  margin-right: 6px;
  overflow: hidden;
  text-align: center
}

.sub-menu-list.not-first .sub-menu .more-sub-menu>text:nth-child(3n) {
  margin-right: 0
}

.sub-menu-list.not-first .sub-menu .more-sub-menu>text.on {
  border-color: #f6c8ac;
  color: var(--common-pink-color)
}

.sub-menu-list.not-first .sub-menu .more-sub-menu>text .iconfont {
  color: var(--common-pink-color)
}

.filter {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  max-height: 884rpx;
  width: 100%
}

.filter .menu-box {
  flex-shrink: 1;
  max-height: calc(345px - 65px);
  padding: 18rpx 0 0 32rpx
}

.filter .menu-box .box {
  flex-direction: column;
  width: 100%
}

.filter .menu-box .box .title {
  color: #1f1f1f;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 24rpx;
  mix-blend-mode: normal;
  text-align: justify;
  width: 100%
}

.filter .menu-box .box .labels {
  flex-direction: row;
  flex-wrap: wrap;
  padding-bottom: 40rpx
}

.filter .menu-box .box .labels .on {
  background-color: var(--common-pink-color);
  background: hsla(38, 97%, 88%, .22);
  border: 2rpx solid #fee9c6;
  border-radius: 8rpx;
  box-sizing: border-box;
  color: #fff;
  color: #402e32;
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  font-weight: 450;
  line-height: 28rpx;
  mix-blend-mode: normal;
  text-align: center
}

.filter .menu-box .box .labels>view {
  align-items: center;
  background: #f5f5f5;
  border: 1rpx solid #f5f5f5;
  border-radius: 2px;
  border-radius: 8rpx;
  box-sizing: border-box;
  color: #333;
  flex-direction: row;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  font-size: 26rpx;
  font-weight: 400;
  height: 66rpx;
  justify-content: center;
  line-height: 26rpx;
  margin-top: 24rpx;
  mix-blend-mode: normal;
  text-align: center;
  width: calc((100% - 30rpx*3)/3);
  width: 218rpx
}

.filter .btn-box {
  flex-direction: row !important;
  flex-shrink: 0;
  height: 128rpx;
  justify-content: space-around
}

.filter .btn-box,
.filter .btn-box>view {
  align-items: center
}

.filter .btn-box>view {
  border-radius: 40px;
  border-radius: 16rpx;
  box-sizing: border-box;
  height: 40px;
  justify-content: center;
  mix-blend-mode: normal;
  width: 320rpx
}

.filter .btn-box .reset {
  background: hsla(38, 97%, 89%, .6);
  border: 2rpx solid var(--FEE9C6, hsla(38, 97%, 89%, .85));
  color: #9c705a;
  height: 80rpx;
  width: 274rpx
}

.filter .btn-box .submit {
  background-color: var(--common-pink-color);
  background: var(4.02e+34, #402e32);
  border-radius: 16rpx;
  color: #fff;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  font-weight: 450;
  height: 80rpx;
  line-height: 32rpx;
  margin-left: 20rpx;
  mix-blend-mode: normal;
  text-align: right;
  width: 392rpx
}

.filter>.footer {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  margin: 36rpx 0
}

.filter>.footer .text {
  background: #fef6e7;
  border-radius: 16rpx;
  color: #9c705a;
  font-size: 26rpx;
  line-height: 26rpx;
  padding: 12rpx 64rpx
}

.filter>.footer .highlight {
  color: #ff6d00;
  margin-left: 8rpx;
  text-decoration: underline
}

.mask {
  background-color: initial;
  bottom: 0;
  left: 0;
  position: fixed;
  right: 0;
  top: 0;
  transition: background-color .15s linear;
  z-index: 10
}

.mask.show {
  background-color: rgba(6, 6, 8, .5)
}

.mask.hide {
  display: none
}

@font-face {
  font-family: HM-FD-font;
  src: url(data:application/x-font-woff2;charset=utf-8;base64,)
}

.iconfont {
  color: #757575;
  font-family: HM-FD-font !important;
  font-size: 13px;
  font-style: normal
}

.iconfont.triangle:before {
  content: ""
}

.iconfont.selected:before {
  content: ""
}

.label__container {
  align-items: center !important;
  display: -webkit-flex !important;
  display: flex !important;
  flex-direction: column !important;
  justify-content: center !important;
  margin-right: 16rpx
}

.label__icon {
  font-size: 20rpx;
  margin-right: 4rpx
}

.label__tip {
  color: #acacac;
  font-size: 20rpx
}